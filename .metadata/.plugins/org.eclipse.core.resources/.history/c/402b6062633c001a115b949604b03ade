package oldMacDonald;

public class oldMacDonald
{

	public static void main(String[] args) 
	{
	buildVerse();
		
	pig();	
	
	
	}
	
	public static void buildVerse()
	{
		System.out.println("Old MacDonald had a farm, E-I-E-I-O");
	}
	
	public static void pig()
	{
		System.out.println("And on that farm he had a pig, E-I-E-I-O\r\n" + 
				"	With an oink-oink here and an oink-oink there\r\n" + 
				"	Here an oink, there an oink, everywhere an oink-oink\r\n" + 
				"");
	}
	
	
	
}
	 



