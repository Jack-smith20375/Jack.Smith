package UsingPrintF;

public class printF1 {

	public static void main(String[] args)
	{
	char character = 'h';
	System.out.printf("%s \n", character);
	
	
//	double decimalNumber = 3.14;
//	System.out.printf("%d \n", decimalNumber);
//	
	
	float bigNumber = 0.123f;
	System.out.printf("%.3f \n", bigNumber);
	
	
//	int num = 11;
//	System.out.printf("%i \n", num);
//	
	
	String name = "jack";
	System.out.printf("%S \n", name);
	//%% percent sign
	
	}
}
