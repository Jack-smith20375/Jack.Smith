package UsingPrintF;

public class printF1 {

	public static void main(String[] args)
	{
	char character = 'h';
	System.out.printf("%s \n", character);
	
	
	double decimalNumber = 3.14;
	//System.out.printf("%d", decimalNumber);
	
	
	float bigNumber = 0.123f;
	System.out.printf("%.3f", bigNumber);
	
	
	int num = 10;
	System.out.printf("%i", num);
	
	
	String name = "jack";
	System.out.printf("%S", name);
	//%% percent sign
	
	}
}
